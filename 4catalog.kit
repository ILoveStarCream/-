<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>4Catalog</title>

    <link rel="stylesheet" href="./css/magnific-popup.css">
    <link rel="stylesheet" href="./css/slick-theme.css">
    <link rel="stylesheet" href="./css/slick.css">
    <link rel="stylesheet" href="./css/nouislider.min.css">
    <link rel="stylesheet" href="./css/style.css">
</head>

<body>
    <!-- @import "header.html" -->
    <!-- @import "hidemenu.html" -->
    <div class="container">
        <div class="twopage-about-company-title four-catalog-title">
            <a href="" class="twopage-about-company__link">Главная</a>
            <img src="./img/svg/2pagearrow.svg" alt="arrow" class="twopage-about-company__img">
            <a href="" class="twopage-about-company__link">Каталог</a>
            <img src="./img/svg/2pagearrow.svg" alt="arrow" class="twopage-about-company__img">
            <a href="" class="twopage-about-company__link">Колёса и ролики</a>
        </div>
    </div>

    <section class="selection">
        <div class="container">
            <!-- @import "selection.html" -->
        </div>
    </section>
    <section class="four-catalog">
        <div class="container">
            <div class="four-catalog-wrapper">
                <!-- @import "accordion-menu.html" -->
                <!-- @import "four-catalog-content.html" -->
            </div>
            <ul class="four-catalog-column">
                <li class="four-catalog-number four-catalog-number__active">1</li>
                <li class="four-catalog-number"><a href="#" class="four-catalog__link">2</a></li>
                <li class="four-catalog-number"><a href="#" class="four-catalog__link">3</a></li>
                <li class="four-catalog-number"><a href="#" class="four-catalog__link">4</a></li>
                <li class="four-catalog-number"><a href="#" class="four-catalog__link">5</a></li>
            </ul>
        </div>
    </section>

    <!-- @import "footer.html" -->

    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/nouislider.min.js"></script>
    <script src="js/jquery.maskedinput.min.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script>
        let slider = document.getElementById('range-area');

        noUiSlider.create(slider, {
            start: [20, 80],
            connect: true,
            range: {
                'min': 0,
                'max': 100
            }
            ,
            slide: function (event, ui) {
                $("#amount-area-min").val(ui.values[0]);
                $("#amount-area-max").val(ui.values[1]);
            }
        });

    </script>
    <script src="js/main.js"></script>
</body>

</html>