<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>9Catalog</title>

    <link rel="stylesheet" href="./css/magnific-popup.css">
    <link rel="stylesheet" href="./css/slick-theme.css">
    <link rel="stylesheet" href="./css/slick.css">
    <link rel="stylesheet" href="./css/nouislider.min.css">
    <link rel="stylesheet" href="./css/style.css">
</head>

<body>
    <!-- @import "header.html" -->
    <!-- @import "hidemenu.html" -->
    <div class="container">
        <div class="twopage-about-company-title four-catalog-title">
            <a href="" class="twopage-about-company__link">Главная</a>
            <img src="./img/svg/2pagearrow.svg" alt="arrow" class="twopage-about-company__img five-page__img">
            <a href="" class="twopage-about-company__link">Каталог</a>
            <img src="./img/svg/2pagearrow.svg" alt="arrow" class="twopage-about-company__img five-page__img">
            <a href="" class="twopage-about-company__link">Запчасти для погрузчиков</a>
            <img src="./img/svg/2pagearrow.svg" alt="arrow" class="twopage-about-company__img five-page__img">
            <a href="" class="twopage-about-company__link">Кабины, сиденья и крышки капотов</a>
        </div>
    </div>
    <section class="four-catalog">
        <div class="container">
            <span class="about-company-title four-catalog-subtitle five-catalog-subtitle">Запчасти для
                погрузчиков</span>
            <div class="four-catalog-wrapper">
                <!-- @import "accordion-menu.html" -->
                <div class="five-catalog-content-wrapper">
                    <div class="five-catalog-nav">
                        <ul class="five-catalog-column">
                            <li class="five-catalog-column-item">Сортировать по:</li>
                            <li class="five-catalog-column-item"><a href="#"
                                    class="five-catalog-column__link">Рейтингу</a></li>
                            <li class="five-catalog-column-item"><a href="#"
                                    class="five-catalog-column__link">Популярности</a></li>
                            <li class="five-catalog-column-item">
                                <a href="#" class="five-catalog-column__link five-catalog-column__greylink">Цене
                                    <img src="./img/svg/fivearrow.svg" alt="arrow" class="five-catalog-column__img">
                                </a>
                            </li>
                        </ul>
                        <ul class="five-catalog-column">
                            <li class="five-catalog-column-item">Показать по:</li>
                            <li class="five-catalog-column-item five-catalog-column__activelink">25</li>
                            <li class="five-catalog-column-item"><a href="#" class="five-catalog-number__link">50</a>
                            </li>
                            <li class="five-catalog-column-item"><a href="#" class="five-catalog-number__link">100</a>
                            </li>
                        </ul>
                        <div class="five-catalog-button-wrapper">
                            <button class="five-catalog-button five-catalog-first-button"><img
                                    src="./img/svg/button1.svg" alt="arrow"
                                    class="five-catalog-column__subimg"></button>
                            <button class="five-catalog-button"><img src="./img/svg/button2.svg" alt="arrow"
                                    class="five-catalog-column__subimg"></button>
                        </div>
                    </div>
                    <div class="five-catalog-content six-catalog-content">
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                        <!-- @import "six-bestseller.html" -->
                    </div>
                    <ul class="four-catalog-column">
                        <li class="four-catalog-number four-catalog-number__active">1</li>
                        <li class="four-catalog-number"><a href="#" class="four-catalog__link">2</a></li>
                        <li class="four-catalog-number"><a href="#" class="four-catalog__link">3</a></li>
                        <li class="four-catalog-number"><a href="#" class="four-catalog__link">4</a></li>
                        <li class="four-catalog-number"><a href="#" class="four-catalog__link">5</a></li>
                    </ul>

                </div>
            </div>
            <!-- @import "nine-history.html" -->

        </div>

    </section>


    <!-- @import "footer.html" -->
    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/nouislider.min.js"></script>
    <script src="js/jquery.maskedinput.min.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script>



        // вот старт-------------------------
        let slider = document.getElementById('range-area');

        noUiSlider.create(slider, {
            start: [20, 80],
            connect: true,
            range: {
                'min': 0,
                'max': 100
            }
            ,
            slide: function (event, ui) {
                $("#amount-area-min").val(ui.values[0]);
                $("#amount-area-max").val(ui.values[1]);
            }
        });

        // вот конец---------------------------

    </script>
    <script src="js/main.js"></script>
</body>

</html>